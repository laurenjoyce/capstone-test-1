<body hoe-navigation-type="vertical" hoe-nav-placement="left" theme-layout="wide-layout" ng-app="app">

  <!--side navigation start-->
  <div id="hoeapp-wrapper" class="hoe-hide-lpanel" hoe-device-type="desktop">
    <header id="hoe-header" hoe-lpanel-effect="shrink">
      <div class="hoe-left-header">
        <a href="/#"> B<span>E WELL</span></a>
        <span class="hoe-sidebar-toggle"><a href="/#"></a></span>
      </div>

      
    </header>


    <div id="hoeapp-container" hoe-color-type="lpanel-bg7" hoe-lpanel-effect="shrink">
      <aside id="hoe-left-panel" hoe-position-type="absolute">
        <ul class="nav panel-list">
          <li>
            <a href="/">
              <i class="fa fa-home"></i>
              <span class="menu-text">Home</span>
              <span class="selected"></span>
            </a>
          </li>
          <% if current_user == nil %>
          <li><a href="/users/sign_up">Sign Up</a></li>
          <li><a href="/users/sign_in">Log In</a></li>
          <% else %> 
            <% if !current_user.user_profile %>
              <li>Welcome, new user!</li>
            <% else %>
              <li>Welcome, <%= current_user.user_profile.first_name %>!</li>
              <li><a href="/saved_doctors">My Saved Doctors</a></li>
              <li><a href="/user_profiles">My Profile</a></li>
              <li><%= link_to "Log Out", "/users/sign_out", method: :delete %></li>
            <% end %>
          <% end %>
        </ul>
      </aside>
      <section id="main-content" class="container">
        <div>
        <%= yield %>
        </div>
      </section>
    </div>
  </div>



  <!--back to top-->
  <a href="/#" class="scrollToTop"><i class="fa fa-angle-up"></i></a>
  <!--back to top end-->
  <!--side navigation end-->
