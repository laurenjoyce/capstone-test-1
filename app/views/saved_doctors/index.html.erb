<h4><a href="/">Back to search page</a></h4>

<h1>My Saved Doctors</h1>

<% @saved_doctors.each do |doctor| %>
  
  <%= link_to  "#{doctor[:first_name]} #{doctor[:last_name]}", "/doctors/#{doctor[:uid]}" %>

 <p><img src="<%= doctor[:image_url] %>"></p>
  <p><%= doctor[:uid] %></p>

<%= link_to "Remove this doctor from your saved list", "/saved_doctors?doctor_uid=#{doctor[:uid]}", method: :delete %><br/>


<% end %>